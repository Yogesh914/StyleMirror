{
  "expo": {
    "name": "StyleMirror",
    "slug": "StyleMirror",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F1E1C3"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F1E1C3"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }, 
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Ephesis/Ephesis-Regular.ttf"
          ]
        }
      ]
    ]
  }
}
